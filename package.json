{
  "name": "precommit-prettying",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run lint && npm run format",
    "format": "prettier \"**/src/*.js\" -l",
    "lint": "eslint \"**/src/*.js\" --quiet",
    "eslint:init": "eslint --init",
    "test": "echo \"Doing the testing thing\"",
    "kosher": "npm run lint && npm run format && npm run test"
  },
  "lint-staged": {
    "**/src/*.js": ["eslint --quiet --fix", "prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glvangorp/precommit-prettying.git"
  },
  "author": "",
